{
  "manifest_version": 3,
  "name": "bookmark filer",
  "version": "0.1.0",
  "description": "bookmark filer like vim keybindings",
  "permissions": [
    "bookmarks",
    "favicon",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/index.jsx",
        "src/content/components/content.jsx"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  }
}
