{
  "manifest_version": 3,
  "name": "bookmark filer",
  "version": "0.1.1",
  "description": "bookmark filer like vim keybindings",
  "permissions": ["bookmarks", "favicon", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/index.jsx", "src/content/components/content.jsx"]
    }
  ],
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  }
}
