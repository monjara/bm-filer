{
  "manifest_version": 3,
  "name": "bookmark filer",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "bookmarks",
    "favicon"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/index.jsx",
        "src/content/components/content.jsx"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  }
}
